// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Survey {
  id    Int     @id @default(autoincrement())
  name  String
  description String
  type String?
  sub_type String?
  // profileImage String?
  status String?
  visible Boolean?
  enabled Boolean
  user User? @relation(fields: [userId], references: [id])
  userId Int?
}
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String
  userName String
  birthDate String
  password String ?
  phone  String?
  profileImage String?
  status String?
  lastSeen String?
  surveys Survey[]
}

